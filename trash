@dp.message_handler(state=Order.rick_roll)
async def rick_roll(message: types.Message, state: FSMContext):
    await bot.send_video(message.chat.id, "https://c.tenor.com/VFFJ8Ei3C2IAAAAM/rickroll-rick.gif", None, "Text")